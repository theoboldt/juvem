<div class="col-md-12">
    <div id="bootstrap-table-toolbar-participations" class="form-inline">{% block toolbar %}{% endblock %}</div>
    <table id="groupParticipationsTable" class="table table-striped table-remote-content"
           data-id-field="pid" data-toggle="table" data-cache="false" data-pagination="true"
           data-side-pagination="client" data-show-refresh="true" data-show-columns="true"
           data-search-align="right" data-toolbar="#bootstrap-table-toolbar-participations" data-striped="true"
           data-show-filter="true" data-search="true" data-page-size="25" data-sub-id="e{{ event.eid }}"
           data-url="{{ path('admin_event_group_participation_data', {'eid': event.eid, 'bid': choiceOption.attribute.bid, 'cid': choiceOption.id}) }}"
           data-sort-name="nameLast" data-sort-order="asc">
        <thead>
        <tr>
            <th data-field="pid" data-sortable="true" data-visible="false">#</th>
            <th data-field="nameFirst" data-sortable="true">Vorname</th>
            <th data-field="nameLast" data-sortable="true">Nachname</th>
            {% for attribute in event.getAcquisitionAttributes(true, false, false, true, true) %}
                <th data-field="acq_field_{{ attribute.getBid }}" data-sortable="true"
                    data-visible="false">
                    <abbr title="{{ attribute.managementDescription }}">{{ attribute.managementTitle }}</abbr>
                </th>
            {% endfor %}
        </tr>
        </thead>
    </table>
</div>
