{
    "name": "theoboldt/juvem",
    "license": "MIT",
    "type": "project",
    "description": "A web application to manage events, participants and newsletter subscriptions",
    "authors": [
        {
            "name": "Erik Theoboldt",
            "email": "erik@theoboldt.eu"
        }
    ],
    "autoload": {
      "psr-4": {
        "": "src/"
      },
      "classmap": [
        "src/AppKernel.php"
      ]
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "require": {
        "php": ">=7.4",
        "ext-zip": "*",
        "ext-exif": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-xmlwriter": "*",
        "ext-xsl": "*",
        "ext-fileinfo": "*",
        "symfony/symfony": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "symfony/swiftmailer-bundle": "^3.5",
        "swiftmailer/swiftmailer": "^6.1",
        "monolog/monolog": "^1",
        "symfony/monolog-bundle": "^3.6",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",
        "incenteev/composer-parameter-handler": "~2.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "vich/uploader-bundle": "^1.8",
        "friendsofsymfony/user-bundle": "^2.0",
        "misd/phone-number-bundle": "^1.3",
        "imagine/imagine": "^1.2",
        "knplabs/knp-markdown-bundle": "^1.6",
        "nelmio/security-bundle": "^2",
        "twig/twig": "^2",
        "knplabs/knp-time-bundle": "^1.7",
        "yellowskies/qr-code-bundle": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "phpoffice/phpword": "^0.17",
        "phpoffice/phpspreadsheet": "^1.9",
        "jms/serializer-bundle": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/phpunit-bridge": "4.4.* >=4.1.12"
    },
    "scripts": {
        "test": "bin/phpunit",
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "bin-dir": "bin"
    },
    "extra": {
        "symfony": {
          "allow-contrib": false,
          "require": "4.4.*"
        },
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        }
    }
}
