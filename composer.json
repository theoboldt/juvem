{
    "name": "theoboldt/juvem",
    "license": "MIT",
    "type": "project",
    "description": "A web application to manage events, participants and newsletter subscriptions",
    "authors": [
        {
            "name": "Erik Theoboldt",
            "email": "erik@theoboldt.eu"
        }
    ],
    "autoload": {
      "psr-4": {
        "": "src/"
      },
      "classmap": [
        "app/AppKernel.php",
        "app/AppCache.php"
      ]
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "require": {
        "php": ">=7.2",
        "ext-zip": "*",
        "ext-exif": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-xmlwriter": "*",
        "ext-xsl": "*",
        "ext-fileinfo": "*",
        "symfony/symfony": "3.4.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "symfony/swiftmailer-bundle": "^2",
        "swiftmailer/swiftmailer": "<6.0",
        "monolog/monolog": "^1",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensio/generator-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "vich/uploader-bundle": "^1.8",
        "friendsofsymfony/user-bundle": "^2.0",
        "misd/phone-number-bundle": "^1.3",
        "imagine/imagine": "^1.2",
        "knplabs/knp-markdown-bundle": "^1.6",
        "nelmio/security-bundle": "^2",
        "twig/twig": "^2",
        "knplabs/knp-time-bundle": "^1.7",
        "yellowskies/qr-code-bundle": "^1.2",
        "ocramius/proxy-manager": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "phpoffice/phpword": "^0.17",
        "phpoffice/phpspreadsheet": "^1.9",
        "jms/serializer-bundle": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/web-server-bundle": "^3.3"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "optimize-autoloader": true,
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
